@InProceedings{Khatchadourian2010,
  author    = {Khatchadourian, Raffi and Muskalla, Benjamin},
  title     = {Enumeration Refactoring: A Tool for Automatically Converting {Java} Constants to Enumerated Types},
  booktitle = ase,
  series    = {ASE '10},
