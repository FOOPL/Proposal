  date-modified = {2015-09-24 11:37:33 +0000},
  doi           = {10.1145/1218776.1218795},
  journaltitle  = {SIGSOFT Softw. Eng. Notes},
  location      = {New York, NY, USA},
  publisher     = {ACM},
}

